{
	"title": "{{ SITESHORTNAME }}",
	"description": "{{ SITESHORTDESCRIPTION }}",
	"articles" : [
	{% for article in articles %}{% if loop.index < 6 %}
		{
			"title": "{{ article.title }}",
			"link": "{{ SITEURL }}/{{ article.url }}",
			"date": "{{ article.locale_date }}"
		}{% if not loop.last and loop.index < 5 %}, {% endif %}
	{% endif %}{% endfor %}]
}